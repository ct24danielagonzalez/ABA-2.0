// Función para mostrar mensajes en la OLED con centrado correcto
void mostrarMensajes(String mensaje) {
    u8g2.clearBuffer();
    u8g2.setFont(u8g2_font_ncenB10_tr);  // Fuente más grande

    int maxCharsPerLine = 12;  // Máx caracteres por línea en fuente más grande
    int numLines = (mensaje.length() + maxCharsPerLine - 1) / maxCharsPerLine;

    int startY = (64 - (numLines * 14)) / 2;  // Centrar verticalmente

    for (int i = 0; i < numLines; i++) {
        String linea = mensaje.substring(i * maxCharsPerLine, (i + 1) * maxCharsPerLine);
        int textWidth = u8g2.getUTF8Width(linea.c_str());  // Obtener ancho del texto
        int xPos = (128 - textWidth) / 2;  // Calcular centrado horizontal
        u8g2.setCursor(xPos, startY + i * 14);
        u8g2.print(linea);
    }

    u8g2.sendBuffer();  // Mostrar en pantalla
    delay(500);
}

// Función para esperar la presión de un botón
void esperarBoton() {
  bool botonPresionado = false;
  
  // Esperar hasta que cualquiera de los botones se presione
  while (!botonPresionado) {
  mostrarMensajes("Presiona uno   de los    botones");
    delay(500); // Mostrar mensaje durante 500ms

    // Borrar pantalla para hacer el efecto de parpadeo
    u8g2.clearBuffer();
    u8g2.sendBuffer(); 
    delay(500); // Mantener la pantalla en blanco durante 500ms

    for (int i = 0; i < 5; i++) {
      if (digitalRead(botones[i]) == HIGH) { // Si algún botón es presionado
        botonPresionado = true;
        break;
      }
    }
  }
}


// Función para dibujar el oso con ojos abiertos
void dibujarOsoOjosAbiertos() {
    u8g2.clearBuffer();  // Limpiar pantalla

    // Cara del oso
    u8g2.drawCircle(64, 32, 20);  // Cara (círculo grande)

    // Oídos
    u8g2.drawCircle(48, 18, 6);   // Oreja izquierda
    u8g2.drawCircle(80, 18, 6);   // Oreja derecha

    // Ojos abiertos
    u8g2.drawCircle(56, 30, 2);  // Ojo izquierdo
    u8g2.drawCircle(72, 30, 2);  // Ojo derecho

    // Nariz y boca
    u8g2.drawCircle(64, 38, 3);        // Nariz (círculo pequeño)
    u8g2.drawLine(64, 41, 64, 45);     // Línea vertical desde la nariz
    u8g2.drawArc(64, 46, 6, 0, 180);   // Boca (arco hacia abajo)

    u8g2.sendBuffer();  // Enviar al display
}

// Función para dibujar el oso guiñando un ojo
void dibujarOsoGuiñando() {
    u8g2.clearBuffer();  // Limpiar pantalla

    // Cara del oso
    u8g2.drawCircle(64, 32, 20);  // Cara (círculo grande)

    // Oídos
    u8g2.drawCircle(48, 18, 6);   // Oreja izquierda
    u8g2.drawCircle(80, 18, 6);   // Oreja derecha

    // Ojos (uno abierto, uno guiñando)
    u8g2.drawCircle(56, 30, 2);  // Ojo izquierdo abierto
    u8g2.drawLine(70, 30, 74, 30);  // Ojo derecho guiñando (línea horizontal)

    // Nariz y boca
    u8g2.drawCircle(64, 38, 3);        // Nariz (círculo pequeño)
    u8g2.drawLine(64, 41, 64, 45);     // Línea vertical desde la nariz
    u8g2.drawArc(64, 46, 6, 0, 180);   // Boca (arco hacia abajo)

    u8g2.sendBuffer();  // Enviar al display
}

